{
    "parser": "babel-eslint",
    "plugins": [
        "react"
    ],
    "env": {
        "browser": true,
        "node": true
    },
    "extends": ["airbnb/base", "plugin:react/recommended"],
    "rules": {
        "indent": [2, 4],
        "max-len": [1, 120, 2, {"ignoreComments": true}],
        "prop-types": [2]
    }
}
