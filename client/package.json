{
    "name": "mobile-sensor-pd-client",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "watch": "yarn start:dev",
        "start:dev": "yarn dev:path:base npm run dev:path:asset npm run dev:path:cdn npm run dev:path:host npm run dev:path:app webpack-dev-server",
        "build": "HOST=$HOST BASE_URL=$BASE_URL ASSETS_VERSION=$ASSETS_VERSION CDN_URL=$CDN_URL webpack --config webpack.prod.js",
        "test": "jest --verbose",
        "test:clear:snapshots": "rm -r ./__tests__/components/__snapshots__",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
        "dev:path:host": "HOST=",
        "dev:path:base": "BASE_URL=",
        "dev:path:cdn": "CDN_URL=",
        "dev:path:asset": "ASSETS_VERSION=",
        "dev:path:app": "APP_URL="
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
